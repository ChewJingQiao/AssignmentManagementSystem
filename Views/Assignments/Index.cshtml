@model IEnumerable<AssignmentManagementSystem.Models.Assignment>

@{
    ViewData["Title"] = "Index";
}

<h1>Assignments</h1>

<table border="1">
    <tr>
        <th>Assignment ID</th>
        <th>Assignment Name</th>
        <th>Handout Date</th>
        <th>Submission Date</th>
        <th>Module Name</th>
    </tr>
    @foreach (var singledatarow in Model)
    {
        <tr>
            <td>@singledatarow.AssignmentId</td>
            <td>@singledatarow.AssignmentName</td>
            <td>@singledatarow.handoutDate</td>
            <td>@singledatarow.submissionDate</td>
            <td>@singledatarow.Module.ModuleName</td>
        </tr>
    }

</table>